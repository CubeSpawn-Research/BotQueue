{
	"name": "Hoektronics/botqueue",
	"description": "Control your 3D Printer over the Internet",
	"minimum-stability": "dev",
	"license": "GPL-3.0",
	"authors": [
		{
			"name": "Zach 'Hoeken' Smith",
			"email": "hoeken@gmail.com"
		},
		{
			"name": "Justin Nesselrotte",
			"email": "jnesselr@harding.edu"
		}
	],
	"type": "project",
	"require": {
		"laravel/framework": "5.1.*",
		"doctrine/dbal": "^2.6@dev",
		"illuminate/html": "^5.0@dev",
		"dingo/api": "1.0.*@dev"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"barryvdh/laravel-ide-helper": "^2.1@dev",
		"fzaninotto/faker": "~1.4"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		},
		"files": [
			"app/Helpers/helpers.php"
		]
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php",
			"tests/AuthTestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan ide-helper:generate",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
